<!DOCTYPE html>
<html>
  <head>
    <title>Events</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <link href="<%= image_path("favicon.ico") %>" rel="shortcut icon" />
    
    <%= stylesheet_link_tag    'application', media: 'all',
      'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application',
      'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div class="wrapper">
      <div class="content">
        <%= render "layouts/header" %>
        <%= render "layouts/flash" %>
        <%= yield %>
      </div>
    </div>
  </body>
</html>
